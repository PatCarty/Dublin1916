        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.helloworld"
        version   = "1.0.0">
        
        <!-- plugins -->
        
<plugin name="com.phonegap.plugin.statusbar" spec="1.1.0" source="pgb" />
